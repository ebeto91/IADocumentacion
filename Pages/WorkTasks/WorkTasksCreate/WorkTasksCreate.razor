@page "/gestionCiudadana/solicitudes/internas/crear/{redirect}"
@using RAS823_MC_CiudadMunicipal_FrontEnd.Dto.Department
@using RAS823_MC_CiudadMunicipal_FrontEnd.Pages.ControlInternalManagement.ControlInternalManagementEdit.ControlInternalManagementEditModal
@using RAS823_MC_CiudadMunicipal_FrontEnd.Pages.ControlInternalManagement.ControlInternalManagementEdit.ControlInternalManagementEditModalDenied
@using RAS823_MC_CiudadMunicipal_FrontEnd.Pages.Templates.TableFilters
@inherits WorkTasksCreateBase

<div class="container-fluid">

    <!-- ========== title-wrapper start ========== -->
    <div class="title-wrapper pt-30">
        <div class="row align-items-center header-container">
            <div class="col">
                <div class="title container-title">
                    <h3 class="principal-title">
                        Crear Solicitud Interna
                    </h3>
                </div>
            </div>
            <!-- end col -->
            <div class="col">
                <div class="breadcrumb-wrapper">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item">
                                <a>Gestión Ciudadana</a>
                            </li>
                            <li class="breadcrumb-item active" aria-current="page">
                                Solicitudes/ Interna
                            </li>
                        </ol>
                    </nav>
                </div>
            </div>
            <!-- end col -->
        </div>
        <!-- end row -->
    </div>
    <!-- ========== title-wrapper end ========== -->
    <!-- Invoice Wrapper Start -->
    <div class="invoice-wrapper">
        <div class="row">
            <div class="col-12">
                <div class="invoice-card card-style mb-30">
                    <div class="form-group item-note">
                        <label>
                            Nota: Esta sección permite crear una tarea directamente sin la necesidad de una solicitud externa, una denuncia o memoria institucional,
                            una vez creado aparecerá en el listado de tareas
                        </label>

                    </div>
                    @if (ModelFirst != null)
                    {
                        <div class="m-2">
                            <Button Text="Volver" Icon="bi bi-arrow-left" @onclick="goToList" />
                        </div>
                        <EditForm Model="@ModelFirst" Context="formContext">
                            <Step @ref="@_stepper">
                                <StepItem Text="" Title="Detalle y procesar">

                                    <DataAnnotationsValidator />
                                    <div class="divider-data mt-2 mb-2"></div>
                                    <div class="text-center">
                                        <h3 class="principal-title">
                                            Detalle de Solicitud
                                        </h3>
                                    </div>

                                    <div class="row">
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>Título Solicitud * </label>
                                                <InputText class="form-control" @bind-Value="ModelFirst.Title"></InputText>
                                                <ValidationMessage For="@(() => ModelFirst.Title)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>Fecha de Creación * </label>
                                                <DateTimePicker IsDisabled="true" Value="ModelFirst.CreatedAt" />
                                            </div>
                                        </div>
                                    </div>



                                    <div class="row">
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>Monto Presupuestado (₡) </label>
                                                <InputText class="form-control" @bind-Value="ModelFirst.ApplicableBudget"></InputText>
                                                <ValidationMessage For="@(() => ModelFirst.ApplicableBudget)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>Punto de contacto (información del lugar)</label>
                                                <Textarea class="form-control" rows="4" @bind-Value="ModelFirst.ContactPoint" />
                                                <ValidationMessage For="@(() => ModelFirst.ContactPoint)" />
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>Distrito *</label>
                                                <Select Items="listDistricts" @bind-Value="ModelFirst.DistrictCustom" OnSelectedItemChanged="OnItemChangedDistrict"></Select>
                                                @* <InputText class="form-control" @bind-Value="ModelFirst.DistrictCustom"></InputText> *@
                                                <ValidationMessage For="@(() => ModelFirst.DistrictCustom)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>Barrio </label>
                                                <Select Items="listNeighborhoods" @bind-Value="ModelFirst.Neighborhood"></Select>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Detalle Solicitud *
                                                </label>
                                                <Textarea class="form-control" rows="4" @bind-Value="ModelFirst.Description" />
                                                <ValidationMessage For="@(() => ModelFirst.Description)" />
                                            </div>
                                        </div>

                                        <div class="mt-2 col-lg-6 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Tipo de tarea
                                                </label>

                                                <Select Items="listWorkTaskType" @bind-Value="ModelFirst.TypeWorkTaskCustom"></Select>
                                                <ValidationMessage For="@(() => ModelFirst.TypeWorkTaskCustom)" />
                                            </div>
                                        </div>

                                        <div class="mt-2 col-12">
                                            <div class="row mt-3 mb-3">
                                                <div class="col-12">
                                                    <div class="form-group">
                                                        <label for="nombre">Activar Ubicación</label>
                                                        @if (!SharedUbicationPerson)
                                                        {
                                                            <div class="form-group item-note mb-3">
                                                                <label>
                                                                    Nota: en caso de dejar la ubicación por defecto se tomará como si no se hubiera seleccionado una ubicación.
                                                                </label>
                                                            </div>
                                                        }

                                                        <div class="buttons-map-information">
                                                            @if (!SharedUbicationPerson)
                                                            {
                                                                <Button Text="Activar Ubicación" Color="Color.Success" Icon="bi bi-geo-alt-fill" @onclick="()=>AskForActualUbication()" />

                                                               
                                                            }
                                                            <Button Text="Mostrar/Ocultar Mapa" Color="Color.Secondary" Icon="bi bi-info" data-bs-toggle="collapse" data-bs-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample" />
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="collapse show mt-2" id="collapseExample">
                                                    <div class="card card-body">
                                                        <div class="col-12 mt-3">
                                                            <div id="map" height="460px" width="100%"></div>
                                                        </div>
                                                        <div class="row">
                                                            <p>Ubicación</p>
                                                            <div class="col-md-6 col-sm-12">
                                                                <div class="form-group">

                                                                    <label for="lat">Latitud</label>
                                                                    <InputText id="lat" @bind-Value="ModelFirst.Latitude" class="form-control"
                                                                               disabled />
                                                                </div>
                                                            </div>

                                                            <div class="col-md-6 col-sm-12">
                                                                <div class="form-group">

                                                                    <label for="long">Longitud</label>
                                                                    <InputText id="long" @bind-Value="ModelFirst.Longitude" class="form-control"
                                                                               disabled />
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                             
                                            </div>
                                        </div>



                                        <div class="mt-2 col-12">
                                            <div class="text-center">
                                                <h3 class="principal-title">
                                                    Evidencias
                                                </h3>
                                            </div>

                                            <div class="row custom-buttons">
                                                <div class="d-flex-center col-12 mt-2 mb-2">
                                                    <div class="containerInput">
                                                        <InputFile style="display:none !important;" class="noShow" id="inputFile" OnChange="OnInputFileChange" multiple accept="image/*"></InputFile>
                                                        <label for="inputFile">
                                                            <i class="bi bi-cloud-arrow-up"></i> &nbsp; Seleccionar nuevo(s)
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>

                                            <div class="containerMainImages">
                                                @foreach (var imgE in imageDataUrl)
                                                {


                                                    <div id="container-@imgE.Identificador" class="containerImage">
                                                        <i @onclick='()=>HandleDeleteImage(imgE.Identificador)' class="closeCustomIcon bi bi-x-circle"></i>
                                                        <img class="customImg" src="@imgE.imageUrl" />
                                                    </div>
                                                }
                                            </div>

                                        </div>
                                    </div>

                                    <div class="divider-data mt-2 mb-2"></div>

                                    <div class="text-center">
                                        <h3 class="principal-title">
                                            Información Personal
                                        </h3>
                                    </div>
                                    <div class="divider-data mt-2 mb-2"></div>
                                    <div class="text-center">
                                        <h3>
                                            Creado por
                                        </h3>
                                    </div>

                                    <div class="row">
                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>Cédula </label>
                                                <InputText class="form-control disable" @bind-Value="citizenManagmentDto.Identification" readonly></InputText>
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>Nombre Completo </label>
                                                <InputText class="form-control disable" @bind-Value="ModelFirst.ManagementName" readonly></InputText>
                                            </div>
                                        </div>

                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>Correo Electrónico </label>
                                                <InputText class="form-control disable" @bind-Value="ModelFirst.ManagementEmail" readonly></InputText>
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>Teléfono </label>
                                                <InputText class="form-control disable" @bind-Value="ModelFirst.ManagementPhone" readonly></InputText>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="text-center mt-3">
                                        <h3>
                                            Participantes Externos
                                        </h3>
                                    </div>

                                    <div class="row">
                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Identificación
                                                </label>
                                                <InputText class="form-control" @bind-Value="citizenManagmentDto.IdentificationP"></InputText>
                                                <ValidationMessage For="@(() => citizenManagmentDto.IdentificationP)" />
                                            </div>
                                        </div>

                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Nombre Completo
                                                </label>
                                                <InputText class="form-control" @bind-Value="ModelFirst.ExternalManagementName"></InputText>
                                                <ValidationMessage For="@(() => ModelFirst.ExternalManagementName)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Correo Electrónico
                                                </label>
                                                <InputText class="form-control" @bind-Value="ModelFirst.ExternalManagementEmail"></InputText>
                                                <ValidationMessage For="@(() => ModelFirst.ExternalManagementEmail)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-3 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Teléfono
                                                </label>
                                                <InputText class="form-control" @bind-Value="ModelFirst.ExternalManagementPhone"></InputText>
                                                <ValidationMessage For="@(() => ModelFirst.ExternalManagementPhone)" />
                                            </div>
                                        </div>

                                        <div class="mt-2 col-lg-12 col-sm-12">
                                            <div class="form-group ">
                                                <Button type="button" style="margin-top:10px;" OnClick="SearchCitizen">Añadir Participante</Button>
                                            </div>
                                        </div>
                                     
                                    </div>

                                    <div class="divider-data mt-2 mb-2"></div>

                                    <div class="text-center">
                                        <h3 class="principal-title">
                                            Detalles a rellenar solicitud
                                        </h3>
                                    </div>

                                    <div class="row">
                                        @* cambiar para el de verdad *@
                                        <div class="mt-2 col-lg-4 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Clasificar Tipo Solicitud
                                                </label>
                                                <Select Items="listSelectType" @bind-Value="ModelFirst.TypeApplication"></Select>
                                                <ValidationMessage For="@(() => ModelFirst.TypeApplication)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-4 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Clasificar Estado Solicitud *
                                                </label>
                                                <Select Items="listSelectStatus" @bind-Value="ModelFirst.Status"></Select>
                                                <ValidationMessage For="@(() => ModelFirst.Status)" />
                                            </div>
                                        </div>
                                        <div class="mt-2 col-lg-4 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Clasificar Prioridad *
                                                </label>
                                                <Select Items="listSelectPriority" @bind-Value="ModelFirst.Priority"></Select>
                                                <ValidationMessage For="@(() => ModelFirst.Priority)" />
                                            </div>
                                        </div>
                                    </div>


                                    <div class="row">
                                        <div class="mt-2 col-lg-4 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Detalles Adicionales
                                                </label>
                                                <Textarea class="form-control" rows="4" @bind-Value="ModelFirst.AditionalInformation" />
                                                <ValidationMessage For="@(() => ModelFirst.AditionalInformation)" />
                                            </div>
                                        </div>
                                       @*  <div class="mt-2 col-lg-4 col-sm-12">
                                            <div class="form-group ">
                                                <label>
                                                    Código Oficio Inicio *
                                                </label>
                                                <InputText class="form-control" @bind-Value="ModelFirst.OfficeCodeNumber"></InputText>
                                                <ValidationMessage For="@(() => ModelFirst.OfficeCodeNumber)" />
                                            </div>
                                        </div> *@
                                        @*    <div class="mt-2 col-lg-4 col-sm-12">
                                    <div class="form-group ">
                                    <label>
                                    Código Oficio Resolución
                                    </label>
                                    <InputText class="form-control" @bind-Value="ModelFirst.ResolutionNumber"></InputText>
                                    <ValidationMessage For="@(() => ModelFirst.ResolutionNumber)" />
                                    </div>
                                    </div> *@
                                    </div>
                                    <div class="divider-data mt-2 mb-2"></div>
                                    <div class="container-buttons">
                                        <div class="container-passAssing">
                                            <AuthorizeScopeDirective Scope="NewProject:WorkTask">
                                                <Button Text="Levantar Solicitud" Color="Color.Success" Icon="bi bi-person-up" @onclick="()=>passToAssingManagement(formContext)" />
                                            </AuthorizeScopeDirective>
                                        </div>
                                    </div>


                                </StepItem>
                                <StepItem Text="" Title="Levantar solicitud">
                                    @if (IsSecondPageReadyToLoad)
                                    {
                                        <div class="divider-data mt-2 mb-2"></div>
                                        <div class="text-center">
                                            <h3 class="principal-title">
                                                Levantar solicitud a tarea
                                            </h3>
                                        </div>

                                        <div class="row">
                                            <div class="mt-2 col-lg-6 col-sm-12">
                                                <div class="form-group ">
                                                    <label>Fecha de inicio de tarea * </label>
                                                    @* <DateTimePicker Value="ModelFirst.StartDateApplication" OnValueChanged="@OnItemChanged" /> *@

                                                    <DateTimePicker MinValue="DateTime.Today" Value="ModelFirst.StartDateApplication" OnValueChanged="@OnItemChanged"
                                                                    @ref="_picker1"
                                                                    OnGetDisabledDaysCallback="OnGetDisabledDaysCallback" DisplayDisabledDayAsEmpty="true" />


                                                    <ValidationMessage For="@(() => ModelFirst.StartDateApplication)" />
                                                </div>
                                            </div>
                                            <div class="mt-2 col-lg-6 col-sm-12">
                                                <div class="form-group ">
                                                    <label>Fecha de finalización * </label>
                                                    @* <DateTimePicker IsDisabled="true" Value="ModelFirst.DueRateApplication" /> *@

                                                    <DateTimePicker MinValue="ModelFirst.StartDateApplication" Value="ModelFirst.DueRateApplication"
                                                                    OnGetDisabledDaysCallback="OnGetDisabledDaysCallback"
                                                                    IsDisabled="!ModelFirst.DueRateApplication.HasValue" />
                                                    <ValidationMessage For="@(() => ModelFirst.DueRateApplication)" />
                                                </div>
                                            </div>
                                        </div>

                                        <div class="divider-data mt-2 mb-2"></div>

                                        <div class="text-center">
                                            <h3 class="principal-title">
                                                Asignación usuarios
                                            </h3>
                                        </div>

                                        <div class="row">
                                            <div class="mt-2 col-lg-5 custom-size-col-5 col-sm-12">
                                                <div class="form-group item-note">
                                                    <label>
                                                        Colaboradores
                                                    </label>

                                                </div>
                                                <Table TItem="UserDepartmentDto" @ref=Table PageItemsSource="PageItems" TableSize="TableSize.Normal" RenderMode="TableRenderMode.Table"
                                                       IsPagination="true"
                                                       IsStriped="true" IsBordered="true" OnQueryAsync="@OnQueryAsync"
                                                       ShowSkeleton="true">
                                                    <TableColumns>
                                                        <TableColumn @bind-Field="@context.Id" Width="75" Text="Seleccionar">

                                                            <Template Context="v">
                                                                <div class="w-100 d-flex-center">

                                                                    <input @key="v.Row.Id" class="form-check-input custom-check" type="checkbox" checked="@v.Row.Selected" @oninput="((e) => CheckboxChanged(e,v))">
                                                                </div>
                                                            </Template>
                                                        </TableColumn>
                                                        @* Filterable activar filtros *@
                                                        <TableColumn @bind-Field="@context.Name" Width="100" TextWrap="true" Filterable="false" Text="Nombre">

                                                            <Template Context="value">
                                                                <div class="table-cell is-wrap">
                                                                    @value.Row.Name @value.Row.Lastname
                                                                </div>
                                                            </Template>
                                                        </TableColumn>

                                                        <TableColumn @bind-Field="@context.EmailAddress" Width="175" TextWrap="true" Filterable="true" Text="Correo Colaborador">
                                                            <FilterTemplate>
                                                                <TextFilter Placeholder="Buscar por correo"></TextFilter>
                                                            </FilterTemplate>

                                                            <Template Context="value">
                                                                @if (!string.IsNullOrEmpty(value.Row.EmailAddress))
                                                                {
                                                                    <div class="table-cell is-wrap">
                                                                        @value.Row.EmailAddress
                                                                    </div>
                                                                }
                                                                else
                                                                {
                                                                    <div class="table-cell is-wrap">
                                                                        Sin Asignar
                                                                    </div>
                                                                }
                                                            </Template>
                                                        </TableColumn>

                                                        <TableColumn @bind-Field="@context.DepartmentId" Width="100" TextWrap="true" Filterable="true" Text="Departamento">
                                                            <FilterTemplate>

                                                                <AutoFillFilter Placeholder="Buscar por nombre" ListItems="listDepartmentsSelect"></AutoFillFilter>
                                                            </FilterTemplate>
                                                            <Template Context="value">
                                                                <div class="table-cell is-wrap">
                                                                    @value.Row.NameDepartment
                                                                </div>
                                                            </Template>
                                                        </TableColumn>

                                                    </TableColumns>
                                                </Table>
                                            </div>
                                            <div class="mt-5 col-lg-2   custom-size-col-2 col-sm-12  d-flex-center">
                                                <div class="align-buttons">
                                                    <Button Text="" Icon="bi bi-arrow-right" @onclick="()=>passToFirstToSecondTable()" />


                                                    <Button Text="" Icon="bi bi-arrow-left" @onclick="()=>passToSecondToFirstTable()" />
                                                </div>
                                            </div>

                                            <div class="mt-2 col-lg-5 custom-size-col-5 col-sm-12">
                                                <div class="form-group item-note">
                                                    <label>
                                                        Colaboradores asignados
                                                    </label>
                                                </div>
                                                <Table TItem="UserDepartmentDto" @ref=TableSecond  ShowFooter=true PageItemsSource="PageItems" TableSize="TableSize.Normal" RenderMode="TableRenderMode.Table"
                                                       IsPagination="true"
                                                       IsStriped="true" IsBordered="true" OnQueryAsync="@OnQuerySecondTableAsync"
                                                       ShowSkeleton="true">
                                                    <TableColumns>
                                                        <TableColumn @bind-Field="@context.Id" Width="75" Text="Seleccionar">

                                                            <Template Context="v">
                                                                <div class="w-100 d-flex-center">

                                                                    <input @key="v.Row.Id" class="form-check-input custom-check" type="checkbox" checked="@v.Row.Selected" @oninput="((e) => CheckboxChangedSecondTable(e,v))">
                                                                </div>
                                                            </Template>
                                                        </TableColumn>
                                                        @* Filterable activar filtros *@
                                                        <TableColumn @bind-Field="@context.Name" Width="100" TextWrap="true" Filterable="false" Text="Nombre">
                                                            <FilterTemplate>
                                                                <TextFilter Placeholder="Buscar por nombre"></TextFilter>
                                                            </FilterTemplate>
                                                            <Template Context="value">
                                                                <div class="table-cell is-wrap">
                                                                    @value.Row.Name @value.Row.Lastname
                                                                </div>
                                                            </Template>
                                                        </TableColumn>

                                                        <TableColumn @bind-Field="@context.EmailAddress" Width="175" TextWrap="true" Filterable="false" Text="Correo Colaborador">
                                                            <Template Context="value">
                                                                @if (!string.IsNullOrEmpty(value.Row.EmailAddress))
                                                                {
                                                                    <div class="table-cell is-wrap">
                                                                        @value.Row.EmailAddress
                                                                    </div>
                                                                }
                                                                else
                                                                {
                                                                    <div class="table-cell is-wrap">
                                                                        Sin Asignar
                                                                    </div>
                                                                }
                                                            </Template>
                                                        </TableColumn>

                                                        <TableColumn @bind-Field="@context.NameDepartment" Width="100" TextWrap="true" Filterable="false" Text="Departamento">
                                                            <FilterTemplate>
                                                                <TextFilter Placeholder="Buscar por nombre"></TextFilter>
                                                            </FilterTemplate>
                                                            <Template Context="value">
                                                                <div class="table-cell is-wrap">
                                                                    @value.Row.NameDepartment
                                                                </div>
                                                            </Template>
                                                        </TableColumn>

                                                        <TableColumn @bind-Field="@context.Id" Width="170" Text="Puesto Tarea">
                                                            @* Template Botones *@
                                                            <Template Context="v">
                                                                <div class="custom-button-container">
                                                                    <Select Items="listWorkTaskPosition"
                                                                            @bind-Value="v.Row.Position"></Select>

                                                                </div>
                                                            </Template>
                                                        </TableColumn>

                                                    </TableColumns>
                                                    <FooterTemplate>
                                                        <tr>
                                                            <td colspan="5">
                                                                <div style="text-align: right;">
                                                                    <label> Departamentos asignados</label>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <tr>
                                                            <td colspan="5">
                                                                <div style="text-align: right;">
                                                                    <ul>
                                                                        @foreach (var item in listDepartmentsName)
                                                                        {
                                                                            <li class="font-weight-bold no-bullets">@item</li>
                                                                        }

                                                                    </ul>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                    </FooterTemplate>
                                                </Table>
                                            </div>
                                        </div>

                                        <div class="row mt-5 custom-buttons">

                                            <div class="d-flex-center col-md-6 col-sm-12">
                                                <button type="button" class="back-button button-general-form" @onclick="() => backToDetail()">
                                                    Paso Anterior
                                                </button>
                                            </div>

                                            <div class="d-flex-center col-md-6 col-sm-12">
                                                <Button Text="Asignar Solicitud" Color="Color.Success" Icon="bi bi-person-up" @onclick="()=>completeDataToWorkTask(formContext)" />
                                            </div>



                                        </div>
                                    }



                                </StepItem>

                            </Step>
                        </EditForm>


                    }
                </div>
                <!-- End Card -->
            </div>
            <!-- ENd Col -->
        </div>




    </div>
</div>




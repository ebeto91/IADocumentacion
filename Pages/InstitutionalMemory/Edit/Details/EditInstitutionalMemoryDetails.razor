                                        <Card IsCenter="true">
                                            <BodyTemplate>
                                                <EditWorkTasksDetailsComments @ref=editWorkTasksDetailComponent isViewParam="isView"></EditWorkTasksDetailsComments>
                                            </BodyTemplate>
                                        </Card>
                                    </div>




                                </div>


                            </CollapseItem>

                        </CollapseItems>
                    </Collapse>

                </div>
            </div>

            @* </EditForm> *@

        </BodyTemplate>
    </Card>

                            {
                                <CollapseItem Text="Gestión colaboradores" class="mt-2 mb-2" IsCollapsed="false" Icon="fa-solid fa-users">

                                    <div class="row mb-3">
                                        <div class="mt-2 col-lg-5  custom-size-col-5 col-sm-12">

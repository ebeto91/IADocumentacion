<div class="containerEncuenta">

   <h2>Encuestas</h2>

@if (surveyResponseDataCarrousels.Count > 0)
{
    <div class="conatainerMainCarousel">
        <Carousel  class="customCarousel" style="height:inherit !important" ShowIndicators="true">

            @foreach (var group in surveyResponseDataCarrousels) {
                <CarouselItem>
                    <div class="carousel-item-demo2">
                          <section class="articles">
                        @foreach(var g in group)
                        {

                                    <article>
                                        <div class="article-wrapper">
                                            <figure>
                                                @if (g.PrincipalImage != null)
                                                {
                                                    <img src="@g.PrincipalImage.FilePath" alt="" />
                                                }
                                                else{

                                                    <img src="/sample-data/encuestaDefault.jpg" alt="" />
                                                    }
                                                </figure>
                                            <div class="article-body">
                                                <h2>@g.Title</h2>
                                                <p>
                                                    @g.Description.
                                                </p>
                                                <a href="#" class="read-more">
                                                    Ver mas <span class="sr-only">about this is some title</span>
                                                    <svg xmlns="http://www.w3.org/2000/svg" class="icon" viewBox="0 0 20 20" fill="currentColor">
                                                        <path fill-rule="evenodd" d="M12.293 5.293a1 1 0 011.414 0l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-2.293-2.293a1 1 0 010-1.414z" clip-rule="evenodd" />
                                                    </svg>
                                                </a>
                                            </div>
                                        </div>
                                    </article>
                               
                              
                        }
                            </section>
                        
                    </div>
                </CarouselItem>
            }
           
           
        </Carousel>
    </div>
}
else{

}
</div>

